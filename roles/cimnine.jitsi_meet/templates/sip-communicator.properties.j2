# {{ ansible_managed }}

org.jitsi.videobridge.AUTHORIZED_SOURCE_REGEXP=focus@auth.{{ jitsi_domain }}/.*
org.jitsi.videobridge.TCP_HARVESTER_PORT={{ jitsi_tcp_harvester_port }}

{% if jitsi_nat|bool %}
# NAT configuration
org.ice4j.ice.harvest.NAT_HARVESTER_LOCAL_ADDRESS={{ jitsi_nat_local_ip | default(ansible_default_ipv4['address'], true) }}
org.ice4j.ice.harvest.NAT_HARVESTER_PUBLIC_ADDRESS={{ jitsi_nat_public_ip | default(ipify_public_ip, true) }}
{% endif %}
